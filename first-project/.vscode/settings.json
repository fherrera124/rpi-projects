    "*.COBCOPY*": "cobol",
    "*.COPYBOOK*": "cobol",
    "*.COPY*": "cobol",
    "*.PL1*": "pl1",
    "*.PLI*": "pl1",
    "*.INC*": "pl1",
    "*.INCLUDE*": "pl1",
    "*.JCL*": "jcl",
    "*.ASM*": "hlasm",
    "*.ASSEMBLE*": "hlasm",
    "*.HLASM*": "hlasm",
    "*.HLA*": "hlasm",
    "sdkconfig": "c",
    "stdio.h": "c",
    "ifaddrs.h": "c",
    "if.h": "c",
    "socket.h": "c",
    "inet.h": "c",
    "unistd.h": "c",
    "stdlib.h": "c",
    "module.h": "c"
